{
    "name": "laragrep/laragrep",
    "description": "Laravel package for natural language queries against database metadata.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/routing": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaraGrep\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraGrep\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraGrep\\LaraGrepServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
